{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom","es2020"],
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {"@/*": ["./*"]}
  },
  "include": ["./app", "./components", "./public", "./types"]
}
